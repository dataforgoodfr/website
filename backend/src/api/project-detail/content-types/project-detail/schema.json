    "seo_meta": {
      "type": "component",
      "component": "seo-meta.seo-meta",
      "repeatable": false
    },
    "banner_type": {
      "type": "string"
    },
    "banner_image": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "project": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::project.project"
    },
    "partner_quote_background": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "partner_quote": {
      "type": "component",
      "component": "testimonial.testimonial",
      "repeatable": false
    }
  }
}
