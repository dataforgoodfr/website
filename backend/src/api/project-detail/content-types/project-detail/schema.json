{
  "kind": "singleType",
  "collectionName": "project_details",
  "info": {
    "singularName": "project-detail",
    "pluralName": "project-details",
    "displayName": "ProjectDetail"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
