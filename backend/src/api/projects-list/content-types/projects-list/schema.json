{
  "kind": "singleType",
  "collectionName": "projects_lists",
  "info": {
    "singularName": "projects-list",
    "pluralName": "projects-lists",
    "displayName": "ProjectsList"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "seo_meta": {
      "type": "component",
      "component": "seo-meta.seo-meta",
      "repeatable": false
    },
    "introduction": {
      "type": "text"
    },
    "introduction_cta": {
      "type": "component",
      "component": "call-to-action.call-to-action-with-image",
      "repeatable": false
    },
    "thematics": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::thematic.thematic"
    },
    "seasons": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::season.season"
    },
    "projects": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::project.project"
    },
    "categories": {
      "type": "customField",
      "customField": "plugin::multi-select.multi-select",
      "default": "[]",
      "options": [
        "Plaidoyer",
        "Outil Interne",
        "Outil pour les citoyens"
      ]
    },
    "join_cta": {
      "type": "component",
      "component": "call-to-action.call-to-action",
      "repeatable": false
    },
    "informations_title": {
      "type": "string"
    },
    "informations": {
      "type": "component",
      "component": "information.information-block",
      "repeatable": true
    }
  }
}
